#include <iostream>
#include <vector>
using namespace std;
int main() {
    int N;
    cin >> N;
    int a = 0;
    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < N; ++j) {
            int b;
            cin >> b;
            a += b;
        }
    }
    cout << a / 2 << '\n';
    return 0;
}
