/*
Дана последовательность целых чисел, по модулю не превосходящих 100. Найти в ней наименьшее число.

Входные данные
Сначала задано число N - количество чисел в последовательности, а затем N чисел (1 ≤ N ≤ 100).

Выходные данные
Выведите наименьшее число.
*/


#include <iostream>
using namespace std;
 
int main() {
 
	int n{}; // количество
	int a[100]{};
	cin >> n;
	int b{};
	int namber{};
	for (size_t i = 0; i < n; i++){
		cin >> b;
		a[i] = b;
	}
	b = a[0];
	for (size_t i = 0; i < n; i++) {
		if (a[i] < b) {
			b = a[i];
		}
	}
	cout << b << endl;
 
	return 0;
}
