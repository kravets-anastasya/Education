/*
Даны три числа. Необходимо найти значение наибольшего из них.
*/


#include <iostream>
using namespace std;
 
int main() {
 
    int number1{};  
    int number2{};  
    int number3{};  
	int max{};
    cin >> number1; 
    cin >> number2; 
    cin >> number3; 
    
	if (number1 > number2 && number1 > number3) {
		max = number1;
	}
	else if (number2 > number3 ) {
		max = number2;
	}
	else {
		max = number3;
	}
	cout << max << endl;
	
    return 0;
}
