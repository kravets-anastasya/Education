/*
Даны координаты двух полей шахматной доски. Координаты клетки - это два числа от 1 до 8: номер столбца и номер строки. Одного ли цвета эти клетки на шахматной доске?

Входные данные
В первой строке заданы: столбец первого поля, затем строка первого поля, затем столбец второго поля, затем строка второго поля.

Выходные данные
Вывести сообщение "YES", если они одного цвета, и "NO" - иначе.
*/


#include <iostream>
using namespace std;
 
int main() {
 
    int number1{};  // номер столбца
    int number2{};  //номер строки
    int number3{};  // номер столбца
    int number4{};  //номер строки
    cin >> number1; // номер столбца
    cin >> number2; //номер строки
    cin >> number3; // номер столбца
    cin >> number4; //номер строки
    if (number1 < 1 || number1 > 8 && number2 < 1 || number2 > 8 && number3 < 1 || number3 > 8 && number4 < 1 || number4 > 8) {
        return -1;
    }
    if ((number1 + number2) % 2 == (number3 + number4) % 2) {
        cout << "YES"; 
    }else {
        cout << "NO";
    }
	
    return 0;
}
